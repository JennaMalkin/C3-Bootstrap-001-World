scrp 2 13 6 9
	addv ov01 1
	subv ov02 1
	addv ov20 1

	doif ov20 > 200 and ov00 = 0
		setv ov00 6
	endi

	doif ov02 < ov73
		setv ov00 1
	endi


	doif ov02 <= 0 or ov01 > 2000
		setv ov00 99
	endi

	setv va60 prop grid ownr left 5
	setv va61 prop grid ownr rght 5
	doif va60 > va61
		setv ov10 1
	elif va60 < va61
		setv ov10 -1
	endi

	doif rtyp room ownr = 8
		subv ov02 50
	endi

	doif obst 0 lt 30
		setv ov10 1
		setv ov11 -1
	endi
	doif obst 1 lt 30
